<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <corners android:radius="6dp"/>
            <stroke android:width="1dp" android:color="#00939393" />
            <solid android:color="#0E939393" />

        </shape>
    </item>

    <item android:bottom="2dp"
        android:top="1dp"
        android:end="2dp"
        android:start="2dp">
        <shape
            android:shape="rectangle">
            <stroke android:width="15dp" android:color="#24BCBBBB" />
            <solid android:color="#00BCD4" />
        </shape>
    </item>

    <item android:bottom="5dp" android:top="1dp" android:end="2dp" android:start="2dp"
        >
        <shape
            android:shape="rectangle">
            <corners android:topLeftRadius="1dp"
                android:topRightRadius="1dp"
                android:bottomLeftRadius="1dp"
                android:bottomRightRadius="1dp"/>
            <stroke android:width="1dp" android:color="#FFFFFF"/>
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>

</layer-list>